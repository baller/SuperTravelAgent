{
    "mcpServers": {
        "fetch": {
            "command": "uvx",
            "args": [
                "mcp-server-fetch"
            ],
            "env": {
                "UV_INDEX_URL": "https://pypi.tuna.tsinghua.edu.cn/simple"
            },
            "disabled": false
        },
        "serper_web_search":{
            "sse_url":"http://127.0.0.1:34011/sse",
            "disabled": false
        },
        "baidu-map": {
            "command": "npx",
            "args": [
                "-y",
                "@baidumap/mcp-server-baidu-map"
            ],
            "env": {
                "BAIDU_MAP_API_KEY": "nR3zo3hxVLLq9PNsHQ1uoamNPchBbui3"
            },
            "disabled": false,
            "description": "百度地图MCP服务器，提供地理编码、逆地理编码、地点检索、路线规划等10个地图相关API接口"
        }
    
    }
}